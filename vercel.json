{
  "name": "adtech-toolbox",
  "version": 2,
  "buildCommand": "cd json-explorer && npm install && npm run build",
  "outputDirectory": "json-explorer/dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "github": {
    "silent": false,
    "enabled": true,
    "autoAlias": true,
    "deploymentEnabled": {
      "main": true,
      "staging": false
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "staging": false
    }
  }
} 