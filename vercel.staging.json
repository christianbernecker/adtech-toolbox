{
  "name": "adtech-toolbox-staging",
  "version": 2,
  "buildCommand": "cd json-explorer && npm install && npm run build -- --mode staging",
  "outputDirectory": "json-explorer/dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "github": {
    "silent": false,
    "enabled": true,
    "autoAlias": true,
    "deploymentEnabled": {
      "staging": true,
      "main": false
    }
  },
  "env": {
    "VITE_ENVIRONMENT": "staging"
  }
} 